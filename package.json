{
  "name": "eerie",
  "version": "0.0.1",
  "description": "A library that handles user authentication. Provides an API with login/register/password reset requests. Plunks in a database.",
  "main": "eerie.js",
  "scripts": {
    "start": "node eerie.js",
    "db-init": "node ./modules/sequelize-init.js",
    "method-init": "node ./modules/sequelize-methods.js",
    "test": "xo && ava",
    "go": "node listen.js"
  },
  "keywords": [
    "API",
    "Authentication",
    "Login",
    "Register"
  ],
  "author": "Nenad Vuletic",
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.3.1",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "connect-session-sequelize": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "sequelize": "^4.43.0",
    "tedious": "^5.0.3",
    "uuid": "^3.3.2"
  },
  "xo": {
    "rules": {
      "max-params": "off",
      "no-return-await": "off"
    }
  }
}
