{
  "name": "eerie",
  "version": "0.0.1",
  "description": "A library that handles user authentication. Provides an API with login/register/password reset requests. Plunks in a database.",
  "main": "eerie.js",
  "scripts": {
    "start": "node eerie.js",
    "test": "xo && mocha",
    "go": "node listen.js",
    "sloc": "sloc --exclude node_modules ."
  },
  "pre-commit": [
    "test"
  ],
  "keywords": [
    "API",
    "Authentication",
    "Login",
    "Register"
  ],
  "author": "Nenad Vuletic",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^6.0.2",
    "pre-commit": "^1.2.2",
    "supertest": "^4.0.2",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chance": "^1.0.18",
    "connect-redis": "^3.4.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-rate-limit": "^3.4.0",
    "express-session": "^1.15.6",
    "ioredis": "^4.6.2",
    "log4js": "^4.0.2",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "rate-limit-redis": "^1.6.0",
    "readline": "^1.3.0",
    "sequelize": "^4.43.0",
    "tedious": "^5.0.3",
    "uuid": "^3.3.2"
  },
  "xo": {
    "rules": {
      "max-params": "off",
      "no-return-await": "off",
      "import/no-unresolved": "off"
    }
  }
}
